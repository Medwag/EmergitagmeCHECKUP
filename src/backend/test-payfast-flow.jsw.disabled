// test-payfast-flow.jsw
// Quick test function to verify PayFast URL generation
import { generatePayFastUrl } from './payfastUrl.jsw';

export async function testPayFastUrlGeneration() {
  console.log('ğŸ§ª Testing PayFast URL generation...');
  
  const testParams = {
    memberData: {
      firstName: 'Test',
      lastName: 'User',
      email: 'test@example.com'
    },
    pricingPlan: {
      planId: 'test-plan-monthly',
      price: 299.00,
      name: 'Premium Monthly Plan'
    },
    planType: 'monthly'
  };
  
  try {
    console.log('ğŸ“‹ Test parameters:', testParams);
    
    const result = await generatePayFastUrl(testParams);
    console.log('ğŸ“¤ PayFast result:', result);
    
    if (result.success && result.redirectUrl) {
      // Check if URL contains [object Promise]
      if (result.redirectUrl.includes('[object Promise]')) {
        console.error('âŒ FAILED: URL still contains [object Promise]');
        console.error('ğŸ” Problematic URL:', result.redirectUrl);
        return { success: false, error: 'URL contains [object Promise]' };
      } else {
        console.log('âœ… SUCCESS: URL does not contain [object Promise]');
        console.log('ğŸ”— Generated URL preview:', result.redirectUrl.substring(0, 200) + '...');
        
        // Parse URL to verify parameters
        const url = new URL(result.redirectUrl);
        const params = Object.fromEntries(url.searchParams);
        
        console.log('ğŸ“Š URL Parameters:');
        Object.entries(params).forEach(([key, value]) => {
          const hasPromise = value.includes('[object Promise]');
          console.log(`  ${key}: ${value} ${hasPromise ? 'âŒ PROMISE DETECTED' : 'âœ…'}`);
        });
        
        return { 
          success: true, 
          url: result.redirectUrl,
          parameterCount: Object.keys(params).length,
          hasPromises: Object.values(params).some(v => v.includes('[object Promise]'))
        };
      }
    } else {
      console.error('âŒ PayFast URL generation failed:', result.error);
      return { success: false, error: result.error };
    }
  } catch (error) {
    console.error('âŒ Test error:', error);
    return { success: false, error: error.message };
  }
}

export async function quickPayFastTest() {
  console.log('ğŸš€ Quick PayFast Promise Test');
  const result = await testPayFastUrlGeneration();
  return result;
}